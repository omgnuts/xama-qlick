<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" 
		xmlns:local ="clr-namespace:Trak.Client.UI" 
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Trak.Client.UI.DocumentListPage">
	
	<ListView x:Name="listView" HasUnevenRows="true" SeparatorColor="Silver" SeparatorVisibility="None">
	    <ListView.ItemTemplate>
	        <DataTemplate>
	            <local:DocumentCell Self="{Binding .}" />
	        </DataTemplate>
	    </ListView.ItemTemplate>
	</ListView>

</ContentPage>
